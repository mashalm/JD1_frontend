                <div class="col-md-12">
                    <!-- <h2>Find a hearing professional near you</h2> -->
                    <p style="text-align:center">Click on each of the buttons below. If you hear the sound, check the box underneath the button. You may not hear a sound when each button is pressed. Continue to the next page when you have played each sound.</p>

                </div>
            </div>
            <div class="row">
                <BR>
<!--            <BR><BR><BR>-->
                <div class="progress progress-striped active">
                      <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="25"
                      aria-valuemin="0" aria-valuemax="100" style="width:14%" id="puretoneProgress">
                        14%
                      </div>
                    </div>
            </div>
                <div class="row">
                    <div class="col-md-8 col-md-offset-3">
                        <script src="js/puretonetest.js"></script>
                        <a id="tone1" style="padding-right:40px"><i class="fa fa-volume-up fa-4x" aria-hidden="true" onclick="playBeep(125)"></i></a>
                        <a id="tone2" style="padding-right:40px"><i class="fa fa-volume-up fa-4x" aria-hidden="true" onclick="playBeep(500)"></i></a>
                        <a id="tone3" style="padding-right:40px"><i class="fa fa-volume-up fa-4x" aria-hidden="true" onclick="playBeep(1000)"></i></a>
                        <a id="tone4" style="padding-right:40px"><i class="fa fa-volume-up fa-4x" aria-hidden="true" onclick="playBeep(2000)"></i></a>
                        <a id="tone5" style="padding-right:40px"><i class="fa fa-volume-up fa-4x" aria-hidden="true" onclick="playBeep(4000)"></i></a>
                        <a id="tone6" style="padding-right:40px"><i class="fa fa-volume-up fa-4x" aria-hidden="true" onclick="playBeep(8000)"></i></a>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-8 col-md-offset-3" id="checkboxContainer">
                        <input id="check1" type="checkbox" style="margin-left:30px" />
                        <input id="check2" type="checkbox" style="margin-left:100px" />
                        <input id="check3" type="checkbox" style="margin-left:100px" />
                        <input id="check4" type="checkbox" style="margin-left:100px" />
                        <input id="check5" type="checkbox" style="margin-left:100px" />
                        <input id="check6" type="checkbox" style="margin-left:100px" />
                    </div>
                </div>
                <BR>
                <div class="row">
                        <button type="button" class="btn btn-primary" style="float:right" id="puretoneTestNextButton" onclick="setNewGainValue()">Next <i class="fa fa-long-arrow-right fa-lg"> </i></button>
